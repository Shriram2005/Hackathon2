                    function ValidateAlpha(evt)
                    {
                    var keyCode = (evt.which) ? evt.which : evt.keyCode
                    if ((keyCode < 65 || keyCode > 90) && (keyCode < 97 || keyCode > 123) && keyCode != 32 && keyCode != 8 && keyCode != 46)
                    
                    return false;
                    return true;
                    }
                    

                    function validatesendmail(e){
                    $('.srverror').html("");
                    var trimtomail = mailtrim(document.mailform.toaddress.value);
                    if (newsletterwidget.validatemultiuserid(trimtomail) == false) {
                    alert("Please enter a valid email address in - Recipient's mail Field");
                    document.mailform.toaddress.focus();
                    return false;
                    }
                    var trimfrommail = mailtrim(document.mailform.replyto.value);
                    if (newsletterwidget.validateuserid(trimfrommail) == false) {
                    alert("Please enter a valid email address in - Your Email ID");
                    document.mailform.replyto.focus();
                    return false;
                    }
                    var trimyourname = $.trim(document.mailform.fromname1.value);
                    if (trimyourname == 'Your Name' || trimyourname == '' ) {
                    alert("Please enter Your Name");
                    document.mailform.fromname1.focus();
                    return false;
                    }
                    // validate the from_address
                    var trimfrommail = mailtrim(document.mailform.replyto.value);
                    if (checkMail(trimfrommail) == false){
                    alert('Please enter a valid email address in - Your Email ID');
                    document.mailform.replyto.focus();
                    return false;
                    }
                    document.mailform.SuccessURL.value = location.protocol + '//' + location.host + location.pathname +'?thanks=1';
                    document.mailform.FailureURL.value = location.protocol + '//' + location.host + location.pathname +'?failure=1';
                    document.mailform.msid.value = qmsid ;
                    document.mailform.articleurlid.value = location.protocol + '//' + location.host + location.pathname;
                    document.mailform.syn.value = document.mailform.syn.value.replace(/[^a-z0-9\s]/gi, '').replace(/[_]/g, '')+"...";
                    document.mailform.pgtitle.value = document.mailform.pgtitle.value.replace(/[^a-z0-9\s]/gi, '').replace(/[_]/g, '');
                    
                    $.ajax({
                    type : 'POST',
                    url: '/json/postmail.cms',
                    data : $('#mailform').serialize()
                    }).done(function (result) {
                        if (result.postmail.hasOwnProperty('error')) {
                            if(result.postmail.error.xmlns.indexOf("Captcha Not valid") > -1){
                                $('.srverror').html('Please enter a valid captcha').show();
                            } else {
                                $('.srverror').html("Error - " + result.postmail.error.xmlns).show();
                            }
                        } else {
                            $('.postmailsuccess').show().css({'width':$('.mailthisart').width()+10,'height':$('.mailthisart').height()+5});
                            setTimeout(function(){
                                $('#mailform')[0].reset();
                                $('.srverror').html('');
                                $('.postmailsuccess').hide();
                                newsletterwidget.closeconfirm('email-popup')
                            }, 5000);
                        }
                    });
                    return false;
                    }
                    
