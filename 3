                var isMobile = function() {
                  var check = false;
                  (function(a){if(/(android|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|mobile.+firefox|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows ce|xda|xiino/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.substr(0,4))) check = true;})(navigator.userAgent||navigator.vendor||window.opera);
                  return check;
                };
                
                var getQueryParam = function(name, url = window.location.href) {
                    name = name.replace(/[\[\]]/g, '\\$&');
                    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                        results = regex.exec(url);
                    if (!results) return null;
                    if (!results[2]) return '';
                    return decodeURIComponent(results[2].replace(/\+/g, ' '));
                }
                
                var getPlatform = function(queryPlatform){
                    var platform = queryPlatform;
                    if(queryPlatform === 'responsive'){
                        var isFrmapp = getQueryParam('frmapp') === 'yes';
                        platform = isMobile() ? (isFrmapp ? 'app' : 'mweb') : (isFrmapp ? 'webapp': 'web');
                    } 
                    return platform;
                }
                
                
                if(!container){
                    return;
                }
                
                window.__aff_script_promises = window.__aff_script_promises || {};
                
                var loadScript = function(src, attributes = {}){
                  var scriptPromise = window.__aff_script_promises[src];
                
                    if (scriptPromise) {
                        return scriptPromise;
                    }
                      
                    if (window.til_affiliates_sdk) {
                        window.__aff_script_promises[src] = Promise.resolve();
                        return window.__aff_script_promises[src];
                    }
                
                  scriptPromise = new Promise(function(resolve, reject){
                    var script = document.getElementById(src); 
                    if(!script){
                        script = document.createElement('script');
                        script.src = src;
                        script.id = src;
                        document.body.appendChild(script);
                    }
                    
                    script.addEventListener('load', function(){
                      setTimeout(function(){
                        resolve();
                      });
                    });
                    script.addEventListener('error', function(e) {
                      reject(e);
                    });
                    
                  });
                
                  window.__aff_script_promises[src] = scriptPromise;
                  return scriptPromise;
                };
                
                var loadSDKAndBindEvents = function(){
                    // bind to gutter ad show/hide events and
                    // trigger library's corresponding size changed event
                    // this is being done here, as this needs to be done only once
                    // for all affiliate content widgets on the page
                    
                    if(!window.__aff_events_bound){
                        window.addEventListener('GUTTER_AD_TYPE_HANDLING', function(){
                          window.dispatchEvent(new Event('AFFILIATES_CONTENT_SIZE_CHANGED'));
                        });
                        window.addEventListener('GUTTER_CLOSE_AD_TYPE_HANDLING', function(){
                          window.dispatchEvent(new Event('AFFILIATES_CONTENT_SIZE_CHANGED'));
                        });    
                        window.__aff_events_bound = true;
                    }
                    
                    
                    SDKScriptLoadPromise = loadScript(SDK_URL);
                };
                var overridePropMap = {
                    imageurl:'imageURL',
                    affiliatename: 'affiliateName'
                }
                
                 var getComponentPropsFromDataset = function(dataset){
                    var keys = Object.keys(dataset) || [];
                    var config = {}, data = {}
                    var key = '';
                    for(var i = 0; i < keys.length; i++){
                        if(keys[i] && keys[i].includes("config") && dataset[keys[i]]){
                           key = keys[i].replace("config","");
                           key = key.charAt(0).toLowerCase() + key.slice(1)
                           config[key] = dataset[keys[i]]
                        }
                        else if(keys[i] && keys[i].includes("propdata") && dataset[keys[i]]){
                           key = keys[i].replace("propdata","");
                           key = key.charAt(0).toLowerCase() + key.slice(1)
                           key = overridePropMap[key] || key;
                           data[key] = dataset[keys[i]]
                        }
                    }
                    if(dataset['gaLabel']){
                        data.ga ={label:dataset['gaLabel']}
                    }
                    return {data: data, config: config};
                }
                
                var getOptions = function(){
                    var options = {
                      type: dataset['type'],
                      colorTheme: dataset['website'] === "gn" ? "blue" : "",
                      className: dataset['class'],
                      id: dataset['id']
                      
                    };
                    // adding filters
                    options.filters = {
                        website: dataset['website'],
                        platform: getPlatform(dataset['platform']),
                        pcheck: dataset['pcheck'],
                        pagename: dataset['pagename'],
                        keyword: dataset['keyword'],
                        country: (geoInfo || {}).country,
                        affiliatename: dataset['affiliatename'],
                        productid: dataset['productid'],
                        sectionl1: dataset['sectionl1'],
                        sectionmsid: dataset['sectionmsid'],
                        productlistmsid: dataset['productlistmsid'],
                        imgid: dataset['imgid'],
                        addaffiliates: dataset['addAffiliates']
                        
                    };
                    if(!isIframe && dataset['withreferrer'] !== 'no'){
                        options.filters.referrer = document.referrer;
                    }
                    else{
                        options.isAMPIframe= isIframe
                    }
                    
                    
                     options.props = getComponentPropsFromDataset(dataset);
                    
                    // in case we need to fethc additional data for widget , dataurl can be passed as URL
                    if (dataset['dataurl']) {
                       options.dataURL = decodeURIComponent(dataset['dataurl']);
                    }
                    
                    // datapath is the path of the node from where the data has to picked form dataurl response
                    if (dataset['datapath']) {
                       options.dataPath = decodeURIComponent(dataset['datapath']);
                    }
                    
                    //  If data has to be passed to the widget in key other than "data"
                    if(dataset['dataaliaskey']){
                        options.dataAliasKey = decodeURIComponent(dataset['dataaliaskey']);
                    }
                    
                  return options;
                };
                
                var loadWidget = function(){
                    window.til_affiliates_sdk
                    .loadWidget(container, getOptions())
                    .then(function(widgetFeed){
                        isLoading = false
                        
                        var onContentLoadedFn = dataset['onContentLoaded'];
                        
                        if(onContentLoadedFn && typeof window[onContentLoadedFn] === 'function'){
                            window[onContentLoadedFn](widgetFeed);
                        }
                        try{
                            var contentHeight = document.querySelector('.aff_content').scrollHeight
                            if(isIframe && contentHeight >= 100){
                                
                                var message = {
                                    sentinel: 'amp',
                                    type: 'embed-size',
                                    height: contentHeight,
                                };
                                window.parent.postMessage(message, '*');    
                            }
                        }
                        catch(e){
                            
                        }
                    })
                    .catch(function(err){
                        isLoading = false
                        errorOccured = true;
                        
                        var noShowCallbackFn = dataset['noShowCallback'];
                        
                        if(noShowCallbackFn && typeof window[noShowCallbackFn] === 'function'){
                            window[noShowCallbackFn]();
                        }
                        
                        if(isIframe){
                            var message = {
                                sentinel: 'amp',
                                type: 'embed-size',
                                height: '103px',
                            };
                            window.parent.postMessage(message, '*');    
                        }
                    });
                };
                
                var loadContent = function(){
                    SDKScriptLoadPromise.then(function(){
                      if (window.til_affiliates_sdk) {
                        loadWidget();
                      } else {
                        errorOccured = true;
                      }
                    }).catch(function(){
                      errorOccured = true;
                    });
                };
                
                var setInviewObserver = function(){
                    if (initOnPageLoad !=='yes' && 'IntersectionObserver' in window) {
                      var inViewObserver = new IntersectionObserver(
                        function(entries){
                          var entryToCheck = entries[0];
                          if (
                            entryToCheck &&
                            (entryToCheck.isIntersecting || entryToCheck.intersectionRatio > 0)
                          ) {
                            inViewRegistered = true;
                            if(geoInfo && geoInfo.country) {
                                loadContent();
                            }
                            inViewObserver.unobserve(entryToCheck.target);
                          }
                        },
                        { rootMargin: '3000px 0px 3000px 0px' },
                      );
                      inViewObserver.observe(container);
                    } else {
                        inViewRegistered = true;
                        if (geoInfo && geoInfo.country) {
                            loadContent();
                        }
                    }
                }
                
                var setGeoCallback = function() {
                    if (
                      window.TimesGDPR &&
                      window.TimesGDPR.common.consentModule.gdprCallback
                    ) {
                      window.TimesGDPR.common.consentModule.gdprCallback(function(dataObj){
                        geoInfo = {
                        //   country: "US" || geoinfo.CountryCode,
                          country: dataObj.userCountry,
                          continent: dataObj.userContinent,
                          region: dataObj.userRegion,
                        }
                        
                        if(inViewRegistered){
                            loadContent();
                        }
                      });
                    }
                }
                var loadJsFilesAndConfigs = function(){
                    if(window.scrollY > 200){
                        loadSDKAndBindEvents();
                        setInviewObserver();
                        setGeoCallback();
                        window.removeEventListener('scroll', loadJsFilesAndConfigs);
                    }
                }
                
                // init sequence
                // loadSDKAndBindEvents();
                
                if(isIframe){
                    // setInviewObserver(); lazy loading not valid for iframe usecase
                    // in affiliates_content_iframe https://geoapi.indiatimes.com/?cb=1 is loaded in head
                    // sample response ::  var geoinfo = { "CountryCode":"IN","region_code":"DL","city":"NEWDELHI", "Continent":"AS" }
                    // hence the window.geoinfo is assumed to be present before this script would run
                    if(window && window.geoinfo){
                        loadSDKAndBindEvents();
                        geoInfo = {
                        //   country: "AE",
                          country: dataset['devcountry'] || geoinfo.CountryCode,
                          continent: geoinfo.Continent,
                          region: geoinfo.region_code,
                        }
                        // widget Iframe show/hide based on geolocation need to be handled on the parent amp page
                        loadContent();
                    }
                }
                else if(enableWidgetOnLoad==="yes"){
                        loadSDKAndBindEvents();
                        setInviewObserver();
                        setGeoCallback();
                }
                else{
                    
                    window.addEventListener('scroll', loadJsFilesAndConfigs);
                }
            }
        )()
    
    </script><!--/affiliates_content_js.cms?crosshostconfig=83_1&uid=aff-nudge-content85441840&v=9&minify=1potime:64--><!--/affiliates_content.cms?crosshostconfig=83_1&initonpageload=yes&type=floating_sale_nudge&website=timestravel&platform=responsive&uid=aff-nudge-content85441840potime:20--><div class="clear"></div><div class="clear"></div><div xmlns:xslthelper="com.times.utils.xslthelper.XMLHelper" xmlns:math="java.lang.Math" xmlns:java="java" xmlns:ext="http://exslt.org/common" xmlns:dateUtil="com.indiatimes.cms.utilities.CMSDateUtility" class="box1 one-third"><h2><span>Trending Stories</span><span class="arrow"></span></h2><ul class="list2"><li class="clearfix"><a href="/travel/web-stories/your-gujarat-visit-is-incomplete-without-these-experiences/photostory/95445615.cms"><img height="53" width="80" src="https://static.toiimg.com/thumb/msid-95445615,width-80,height-53.cms" alt="" title=""></a><h4><a href="/travel/web-stories/your-gujarat-visit-is-incomplete-without-these-experiences/photostory/95445615.cms">Your Gujarat visit is incomplete without these experiences</a></h4></li><li class="clearfix"><a href="/travel/destinations/fall-in-love-with-karnataka-with-these-photos/photostory/95425797.cms"><img height="53" width="80" src="https://static.toiimg.com/thumb/msid-95425797,width-80,height-53.cms" alt="" title=""></a><h4><a href="/travel/destinations/fall-in-love-with-karnataka-with-these-photos/photostory/95425797.cms">Fall in love with Karnataka with these photos</a></h4></li><li class="clearfix"><a href="/travel/hotels/beautiful-lakeside-retreats-in-india-for-the-traveller-in-you/photostory/95431472.cms"><img height="53" width="80" src="https://static.toiimg.com/thumb/msid-95431472,width-80,height-53.cms" alt="" title=""></a><h4><a href="/travel/hotels/beautiful-lakeside-retreats-in-india-for-the-traveller-in-you/photostory/95431472.cms">Beautiful lakeside retreats in India for the traveller in you</a></h4></li><li class="clearfix"><a href="/travel/destinati