                    function whatsappShare(){
                    var sharetext = document.title;
                   // var sharetext1 = sharetext.replace("&", "and");
                    //var sharetext2 =  sharetext1.replace(/[^a-zA-Z ]/g, "");
                    //var message = sharetext2 ;
                    var UTMparams='?utm_source=whatsapp&utm_medium=social&utm_campaign=TravelPhotostoryicon';
                    var shURL= window.location.href.split('?')[0];
                    var url=shURL+UTMparams;
                     var message = encodeURIComponent(sharetext) + " - " + encodeURIComponent(url);
                     ga('send', 'event', {
                        eventCategory: 'mWEB_photostory_new',
                        eventAction: 'click',
                        eventLabel: 'whatsappFloatingIcon/'+shURL
                      });
                    // Opening URL
                    window.open(
                        "whatsapp://send?text=" + message,
                
                        // This is what makes it 
                        // open in a new window.
                        '_blank' 
                    );
                    
                    
                    }  
                  
